# Environment variables for quick configuration overrides
# Copy to .env and modify as needed

# Redis Configuration
REDIS_ADDR=localhost:6379
REDIS_PASSWORD=
REDIS_DB=0

# Hub Configuration (for hub service)
HUB_GRPC_ADDR=:50051
HUB_HTTP_ADDR=:8080
HUB_CLUSTER_ID=crawler-cluster-1

# Node Configuration (for node service)
NODE_ID=
NODE_HUB_ADDR=localhost:50051
NODE_MAX_WORKERS=10

# Logging
LOG_LEVEL=info
LOG_FORMAT=json

# Monitoring
METRICS_ENABLED=true
METRICS_PORT=:9090