{
  "dashboard": {
    "id": null,
    "title": "Go Backend Kit Monitoring",
    "tags": ["go", "backend", "monitoring"],
    "timezone": "browser",
    "schemaVersion": 16,
    "version": 1,
    "refresh": "5s",
    "time": {
      "from": "now-15m",
      "to": "now"
    },
    "timepicker": {
      "refresh_intervals": ["5s", "10s", "30s", "1m", "5m", "15m", "30m", "1h", "2h", "1d"],
      "time_options": ["5m", "15m", "1h", "6h", "12h", "24h", "2d", "7d", "30d"]
    },
    "panels": [
      {
        "id": 1,
        "title": "HTTP Request Rate",
        "type": "stat",
        "targets": [
          {
            "expr": "rate(http_requests_total[5m])",
            "legendFormat": "{{method}} {{endpoint}}"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 0},
        "options": {
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"],
            "fields": ""
          },
          "orientation": "auto",
          "textMode": "auto",
          "colorMode": "value",
          "graphMode": "area",
          "justifyMode": "auto"
        }
      },
      {
        "id": 2,
        "title": "HTTP Response Time",
        "type": "graph",
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(http_request_duration_seconds_bucket[5m]))",
            "legendFormat": "95th percentile"
          },
          {
            "expr": "histogram_quantile(0.50, rate(http_request_duration_seconds_bucket[5m]))",
            "legendFormat": "50th percentile"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 0},
        "yAxes": [
          {
            "label": "seconds",
            "min": 0
          },
          {
            "show": false
          }
        ]
      },
      {
        "id": 3,
        "title": "Database Connections",
        "type": "graph",
        "targets": [
          {
            "expr": "db_connections_active",
            "legendFormat": "Active - {{database}}"
          },
          {
            "expr": "db_connections_idle",
            "legendFormat": "Idle - {{database}}"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 8},
        "yAxes": [
          {
            "label": "connections",
            "min": 0
          },
          {
            "show": false
          }
        ]
      },
      {
        "id": 4,
        "title": "Database Query Rate",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(db_query_total[5m])",
            "legendFormat": "{{operation}} - {{status}}"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 8},
        "yAxes": [
          {
            "label": "queries/sec",
            "min": 0
          },
          {
            "show": false
          }
        ]
      },
      {
        "id": 5,
        "title": "Redis Command Rate",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(redis_commands_total[5m])",
            "legendFormat": "{{command}} - {{status}}"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 16},
        "yAxes": [
          {
            "label": "commands/sec",
            "min": 0
          },
          {
            "show": false
          }
        ]
      },
      {
        "id": 6,
        "title": "System Metrics",
        "type": "graph",
        "targets": [
          {
            "expr": "cpu_usage_percent",
            "legendFormat": "CPU Usage %"
          },
          {
            "expr": "memory_usage_bytes / 1024 / 1024",
            "legendFormat": "Memory Usage MB"
          },
          {
            "expr": "goroutines_total",
            "legendFormat": "Goroutines"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 16},
        "yAxes": [
          {
            "label": "value",
            "min": 0
          },
          {
            "show": false
          }
        ]
      },
      {
        "id": 7,
        "title": "Error Rate",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(errors_total[5m])",
            "legendFormat": "{{type}} - {{component}}"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 24},
        "yAxes": [
          {
            "label": "errors/sec",
            "min": 0
          },
          {
            "show": false
          }
        ]
      },
      {
        "id": 8,
        "title": "Authentication Attempts",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(authentication_attempts_total[5m])",
            "legendFormat": "{{status}} - {{method}}"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 24},
        "yAxes": [
          {
            "label": "attempts/sec",
            "min": 0
          },
          {
            "show": false
          }
        ]
      },
      {
        "id": 9,
        "title": "Rate Limit Hits",
        "type": "stat",
        "targets": [
          {
            "expr": "rate(rate_limit_hits_total[5m])",
            "legendFormat": "{{endpoint}}"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 32},
        "options": {
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"],
            "fields": ""
          },
          "orientation": "auto",
          "textMode": "auto",
          "colorMode": "value",
          "graphMode": "area",
          "justifyMode": "auto"
        }
      },
      {
        "id": 10,
        "title": "Cache Performance",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(cache_hits_total[5m])",
            "legendFormat": "Hits - {{cache_type}}"
          },
          {
            "expr": "rate(cache_misses_total[5m])",
            "legendFormat": "Misses - {{cache_type}}"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 32},
        "yAxes": [
          {
            "label": "operations/sec",
            "min": 0
          },
          {
            "show": false
          }
        ]
      },
      {
        "id": 11,
        "title": "Active Users",
        "type": "singlestat",
        "targets": [
          {
            "expr": "active_users",
            "legendFormat": "Active Users"
          }
        ],
        "gridPos": {"h": 8, "w": 6, "x": 0, "y": 40},
        "singlestat": {
          "valueName": "current"
        }
      },
      {
        "id": 12,
        "title": "Requests In Flight",
        "type": "singlestat",
        "targets": [
          {
            "expr": "requests_in_flight",
            "legendFormat": "In Flight"
          }
        ],
        "gridPos": {"h": 8, "w": 6, "x": 6, "y": 40},
        "singlestat": {
          "valueName": "current"
        }
      },
      {
        "id": 13,
        "title": "App Uptime",
        "type": "singlestat",
        "targets": [
          {
            "expr": "app_uptime_seconds / 3600",
            "legendFormat": "Hours"
          }
        ],
        "gridPos": {"h": 8, "w": 6, "x": 12, "y": 40},
        "singlestat": {
          "valueName": "current",
          "format": "short"
        }
      },
      {
        "id": 14,
        "title": "External Service Calls",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(external_service_calls_total[5m])",
            "legendFormat": "{{service}} - {{status}}"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 48},
        "yAxes": [
          {
            "label": "calls/sec",
            "min": 0
          },
          {
            "show": false
          }
        ]
      },
      {
        "id": 15,
        "title": "Message Queue Performance",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(message_queue_published_total[5m])",
            "legendFormat": "Published - {{queue}}"
          },
          {
            "expr": "rate(message_queue_consumed_total[5m])",
            "legendFormat": "Consumed - {{queue}}"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 48},
        "yAxes": [
          {
            "label": "messages/sec",
            "min": 0
          },
          {
            "show": false
          }
        ]
      }
    ]
  }
}